{
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon16.png",
    "default_title": "vk messages stats."
  },
  "content_security_policy":
    "script-src 'self' 'unsafe-eval' https://mc.yandex.ru https://vk.com https://ssl.google-analytics.com https://www.google-analytics.com; object-src 'self'",

  "key": "",
  "description": "Статистика личных сообщений и визуализация общения в Вконтакте",
  "manifest_version": 2,
  "name": "VK Messages Visual Statistics",
  "short_name": "VK Visual Stats",

  "minimum_chrome_version": "27",
  "permissions": [
    "tabs", "*://*.userapi.com/*", "*://api.vk.com/*", "*://*.vk.me/*", "*://mc.yandex.ru/*", "*://vk.com/images/*", "storage", "unlimitedStorage"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.1.6"
}